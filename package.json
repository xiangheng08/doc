{
  "name": "doc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm docs:dev",
    "build": "pnpm docs:build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs && node ./script/afterBuild.js",
    "docs:build:gitee": "cross-env LOAD_ENV=gitee_page vitepress build docs",
    "docs:build:server": "cross-env LOAD_ENV=server vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "push": "git add . && git commit -m \"update docs\" && git push",
    "push:server": "npm run docs:build:server && fs send doc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.17.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "markdown-it-mathjax3": "^4.3.2",
    "sass": "^1.80.4",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vitepress": "1.4.1"
  },
  "dependencies": {
    "tdesign-vue-next": "^1.10.2",
    "vue": "^3.5.12"
  }
